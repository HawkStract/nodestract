use capability IO { target: "stdout" }

module ArrayTest

func main() {
    IO.print("--- TEST ARRAY ---")
    
    // 1. Creazione Lista
    stract list = [10, 20, 30]
    IO.print("Lista originale:", list)
    
    // 2. Accesso Indice
    stract first = list[0]
    stract second = list[1]
    IO.print("Elemento 0:", first)
    IO.print("Elemento 1:", second)
    
    // 3. Modifica (Push crea una nuova lista)
    stract newList = Array.push(list, 99)
    IO.print("Nuova lista (push 99):", newList)
    
    // 4. Lunghezza
    stract len = Array.len(newList)
    IO.print("Lunghezza:", len)
}